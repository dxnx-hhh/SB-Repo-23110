{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#article}}  <!-- article을 사용할 범위를 지정한다. 시작태그 # & 종료태그 / -->
    <tr>
        <th scope="row">{{id}}</th>
        <td colspan="2">{{title}}</td>
        <td>{{content}}</td>
    </tr>
    {{/article}}
    </tbody>
</table>

{{>layouts/footer}}